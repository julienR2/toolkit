    "octonode": "^0.10.2",
    "omelette": "^0.4.15-1"
  },
  "devDependencies": {
    "@types/configstore": "^5.0.1",
    "@types/glob": "^7.1.4",
    "@types/inquirer-autocomplete-prompt": "^1.3.2",
    "@types/lodash": "^4.14.172",
    "@types/minimist": "^1.2.2",
    "@types/node": "^16.7.5",
    "@types/omelette": "^0.4.1",
    "cowsay": "^1.4.0",
    "ts-node": "^10.2.1",
    "typescript": "^4.4.2"
